        <nav class="navegador">
            <div class="logo_colpatria">
                <ul>
                    <li><a href="#"><img><span>logo</span></a></li>
                    <li><a href="#">Colpatria scotibank</a></li>
                </ul>
            </div>
            <div class="usuario">
