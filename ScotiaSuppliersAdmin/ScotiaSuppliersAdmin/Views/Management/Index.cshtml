
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Scotia Suppliers Admin</title>
    <link href="~/Design/Management/main.css" rel="stylesheet" />
    <link href="~/Design/Management/reset.css" rel="stylesheet" />
</head>
<body>
    <header>
        <div id="navegador">
            <nav class="navegador">
                <div class="logo_colpatria">
                    <div class="animated2 flash2">
                        <img src="~/Design/Management/Colpatria.png">
                    </div>
                    <div class="animated2 flash">
                        <img src="~/Design/Management/Scotia.png">
                    </div>
                </div>
                <div class="usuario">
                    <ul>
                        <li><a href=""><img src="~/Design/Management/profileimg.png"></a></li>
                        <h1><a href="" class="nombre">ADMINISTRADOR</a></h1>
                    </ul>
                </div>
            </nav>
        </div>
    </header>
    <div class="search">
        <button class="srch"><img src="~/Design/Management/lupa2.png"></button>
        <input type="search" class="inpt">
    </div>
    <section class="selector">
        <div class="contenedor" id="uno">
            <img class="icon" src="~/Design/Management/libreta.png">
            <p class="texto">Provedores</p>
        </div>

        <div class="contenedor" id="dos">
            <img class="icon" src="~/Design/Management/ojo.png">
            <p class="texto">Seguimiento</p>
        </div>
    </section>
    <script src="~/Scripts/jquery-1.8.0.js"></script>
    <script>
        $('#uno, #dos').hover(function () {
            $(this).stop().animate({
                height: "250px",
                maxHeight: "260px"
            });
        }, function () {
            $(this).stop().animate({
                height: "200px",
                maxHeight: "215px"
            });
        });
    </script>
</body>
</html>